<div class="navbar">
  <h4><%= link_to 'Vote BC', :root %></h4>
  <div class="menu-icon">
    <i class="fa fa-bars" aria-hidden="true"></i>
  </div>
  <div class="nav-menu">
   <%= link_to "Districts", districts_path, class: "button" %>
    <div class="dropdown button">
      <i class="fa fa-caret-right up" aria-hidden="true"></i>
      <button class="dropbtn">Elections</button>
      <div class="dropdown-content">
        <% elections.each do |e| %>
        <%= link_to "#{e.year}", "/election/#{e.year}" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
