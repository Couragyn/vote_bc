<%= render 'layouts/parallax' %>
<h1>Election#show</h1>

<p>Find me in app/views/election/show.html.erb</p>
<h1><%= @election.year %></h1>

<br>

<br>
<% @leaders.each do |pl| %>
  <%= pl.name %>
  <%= pl.party.abbr %>
  <% if @premier && pl.id == @premier.id %>
    <%= "(ELECTED!)" %>
  <% end %>
  <br>
<% end %>
<%= render :partial => '/district/district_list', :locals => {districts: @districts} %>
